{
  "name": "vue-data-binder",
  "description": "A helper mixins that associates a 'Vue store state' value with a component. Compatible with Vue 2.7.x.",
  "version": "0.1.0",
  "main": "dist/vue-data-binder.js",
  "module": "dist/vue-data-binder.mjs",
  "unpkg": "dist/vue-data-binder.umd.js",
  "typings": "dist/types/vue-data-binder.type.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bizlinkcorp/vue-data-binder",
  "repository": {
    "type": "git",
    "url": "https://github.com/bizlinkcorp/vue-data-binder"
  },
  "author": "BizLink Co.,Ltd.",
  "keywords": [
    "vue",
    "store bind",
    "vuex",
    "mixin"
  ],
  "scripts": {
    "serve": "vite",
    "build": "npm run build:lib && npm run build:type",
    "build:lib": "vue-tsc --noEmit -p tsconfig.types.json && vite build",
    "build:type": "tsc --emitDeclarationOnly --declaration -P tsconfig.types.json",
    "lint": "eslint \"./{src,example}/**/*.{ts,tsx,vue}\" && prettier --check \"./{src,example}/**/*.{ts,tsx,vue}\"",
    "test:lint": "eslint \"./tests/**/*.ts\" && prettier --check \"./tests/**/*.ts\"",
    "test:unit": "vitest --config ./vitest.config.ts run --coverage"
  },
  "dependencies": {
    "vue": "^2.7.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitest/coverage-c8": "^0.22.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.4.1",
    "typescript": "~4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "vite": "^3.0.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.22.1",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.40.5"
  }
}
